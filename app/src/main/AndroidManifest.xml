<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.segunfrancis.payoneerpaymentmethods">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="com.segunfrancis.payoneerpaymentmethods.PaymentMethodApplication"
        android:allowBackup="true"
        android:icon="@drawable/ic_payoneer"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_payoneer"
        android:supportsRtl="true"
        android:theme="@style/Theme.PayoneerPaymentMethods">
        <activity android:name=".ui.MainActivity" android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>